{
    "manifest_version" : 3,
    "name": "Tab Switcher",
    "version": "1.0",
    "permissions": ["tabs"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.jpeg",
            "48": "icon.jpeg",
            "128": "icon.jpeg"
        }
    }
}